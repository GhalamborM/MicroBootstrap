{
  "app": {
    "name": "game-messaging-service"
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "game-messaging-service",
    "address": "localhost",
    "port": "5007",
    "pingEnabled": false,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "mongo": {
    "connectionString": "mongodb://mongo_test:27017",
    "database": "event-processor-service",
    "seed": false
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "signalR",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "elk": {
    "enabled": false,
    "url": "http://localhost:9200",
    "indexFormat": "game-messaging-service-{0:yyyy.MM.dd}",
    "basicAuthEnabled": false,
    "username": "user",
    "password": "secret"
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "game-messaging-service",
    "requestRetries": 3
  },
  "rabbitMq": {
    "namespace": "messaging",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "redis": {
    "connectionString": "localhost",
    "instance": "messaging:"
  },
  "restEase": {
    "loadBalancer": "",
    "services": []
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  },
  "seq": {
    "enabled": false,
    "url": "http://localhost:5341",
    "token": "secret"
  },
  "signalr": {
    "backplane": "redis",
    "hub": "game"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "SignalR Service",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
    "key": "game-messaging-service/settings",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret"
  }
}