{
  "app": {
    "name": "game-event-processor-service",
    "service": "Game Event Processor Service",
    "displayBanner": true,
    "version": "1"
  },
  "consul": {
    "enabled": true,
    "url": "http://consul:8500",
    "service": "game-event-processor-service",
    "address": "http://game-event-processor-service",
	"port": "7001",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://fabio:9999",
    "service": "game-event-processor-service",
    "requestRetries": 3
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "event-processor",
    "udpHost": "jaeger",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "mongo": {
    "connectionString": "mongodb://mongo:27017",
    "database": "game-event-processor-service",
    "seed": false
  },
  "rabbitMq": {
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "rabbitmq"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "Topic",
      "name": "game-event-sources"
    },
    "queue": {
      "declare": true,
      "autoDelete": false,
      "durable": true,
      "exclusive": false,
      "template": "game-event-processor-service/{{exchange}}.{{message}}"
    }
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  },
  "seq": {
    "enabled": false,
    "url": "http://seq",
    "token": "secret"
  },
  "vault": {
    "enabled": false,
    "url": "http://vault:8200",
    "key": "game-event-processor-service/settings",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v2",
    "title": "Event Processing Service",
    "version": "v1",
    "routePrefix": "docs"
  }
}
