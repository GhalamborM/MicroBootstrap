{
  "app": {
    "name": "game-event-processor-service"
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "game-event-processor-service",
    "address": "localhost",
    "addressForDockerLocalhost": "docker.for.win.localhost",
    "port": "5010",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "game-event-processor-service",
    "requestRetries": 3
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "event-processor",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "mongo": {
    "connectionString": "mongodb://mongo_test:27017",
    "database": "game-event-processor-service",
    "seed": false
  },
  "rabbitMq": {
    "namespace": "event-processor",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "rabbitmq_test"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  },
  "seq": {
    "enabled": false,
    "url": "http://localhost:5341",
    "token": "secret"
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
    "key": "game-event-processor-service/settings",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v2",
    "title": "Event Processing Service",
    "version": "v1",
    "routePrefix": "docs"
  }
